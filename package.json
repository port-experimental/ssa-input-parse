{
  "name": "ssa-inputs-parser",
  "version": "1.0.0",
  "description": "Enterprise-grade CLI tool to parse Port.io Self-Service Action JSON and generate TFVars files",
  "main": "dist/index.js",
  "bin": {
    "ssa-parser": "./bin/run.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepack": "pnpm build",
    "postpack": "rm -f oclif.manifest.json",
    "dev": "ts-node src/index.ts"
  },
  "keywords": [
    "cli",
    "tfvars",
    "port.io",
    "terraform",
    "parser",
    "idp",
    "internal developer portal",
    "self-service action",
    "ssa"
  ],
  "author": "",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@oclif/core": "^3.15.0",
    "@oclif/plugin-help": "^6.0.9",
    "handlebars": "^4.7.8",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "oclif": {
    "bin": "ssa-parser",
    "dirname": "ssa-parser",
    "commands": "./dist/commands",
    "topicSeparator": " "
  }
}
